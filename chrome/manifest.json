{
   "background_page": "video-thumbnails.html",
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "jquery-latest.js" ],
      "matches": [ "http://www.tumblr.com/*", "https://www.tumblr.com/*", "http://tumblr.com/*", "https://tumblr.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "View video thumbnails in your Tumblr dashboard. Developed by the fine folks at Rocketboom and Magma.",
   "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "64": "icon64.png"
   },
   "name": "Tumblr Video Thumbnails",
   "permissions": [ "tabs", "http://*.tumblr.com/*", "https://*.tumblr.com/*", "http://tumblr.com/*", "https://tumblr.com/*" ],
   "update_url": "http://addons.gleuch.com/tumblr-video-thumbnails/updates/chrome",
   "version": "1.0.1"
}
